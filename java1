import java.util.Scanner; /*se não tiver isso não pega */

class hello2 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in); /* no caso o input pode ser qualquer coisa é só */
        int n, n2;                              /* pra salvar o nome do scanner */
        System.out.println("escreva um numero");
        n = input.nextInt();
        System.out.println("escreva outro numero");
        n2 = input.nextInt();
        if (n == n2) {                          /*aparentemente o if é comum e o else também*/
            System.out.println(n + " A igual a B " + n2);
        }
        else {
            System.out.println(n > n2 ? n + " A maior que B " + n2 : n2 + " B maior que A " + n);
        }
    }
}
